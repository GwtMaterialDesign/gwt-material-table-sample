<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='gmdtablesample'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.logging.Logging"/>

    <inherits name="gwt.material.design.GwtMaterialWithJQueryDebug"/>
    <inherits name="gwt.material.design.GwtMaterialTableDebug"/>
    <inherits name="gwt.material.design.addins.GwtMaterialAddinsDebug"/>
    <inherits name="gwt.material.design.jquery.JQuery" />

    <!-- Specify the paths for translatable code -->
    <source path='client'/>
    <source path='shared'/>

    <!-- Specify the app entry point class -->
    <entry-point class='gwt.material.design.sample.client.MaterialTableSample'/>

    <!-- Dev Properties -->
    <set-property name="gwt.logging.logLevel" value="ALL" />
    <set-property name="gwt.logging.developmentModeHandler" value="ENABLED" />

    <set-property name="compiler.stackMode" value="emulated" />
    <set-property name="compiler.emulatedStack" value="true"/>
    <set-property name="compiler.useSourceMaps" value="true" />

    <extend-property name="locale" values="en"/>
    <set-property-fallback name="locale" value="en"/>

    <set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="true"/>
    <set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true"/>
    <set-configuration-property name="compiler.enum.obfuscate.names" value="false" />

    <set-configuration-property name="CssResource.style" value="pretty"/>
    <collapse-all-properties />

    <add-linker name="xsiframe"/>
</module>
